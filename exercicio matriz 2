programa
{
	
	funcao inicio()
	{
		inteiro contador=0
		cadeia cesta[][]={{"joão","sao paulo","(11) 9999-5241"},{"Maria","Ribeirão preto","(16) 9999-8596"},{"Ana","Manaus","(92) 9999-8574"}
		}

				faca {
					escreva ("\n" +"Cliente "+ cesta[contador][0] + " Cidade: " + cesta[contador][1] + " Telefone: "+ cesta[contador][2])
				 	contador++
				}
				enquanto (contador<=2)
		}
		
}
